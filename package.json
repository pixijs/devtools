{
  "name": "@pixi/dev-tools",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "postinstall": "patch-package",
    "clean": "xs clean",
    "lint": "xs lint",
    "lint:fix": "xs lint --fix",
    "types": "xs types",
    "prebuild": "npm run clean",
    "build": "run-p build:*",
    "build:chrome": "vite build --config vite.chrome.config.ts",
    "build:lib": "vite build --config vite.inject.config.ts",
    "postbuild": "mkdir -p .upload && zip -r chrome.zip ./dist/chrome/* && mv chrome.zip .upload/",
    "start": "run-p watch:*",
    "watch": "run-p watch:*",
    "watch:example": "vite dev --config vite.example.config.ts",
    "watch:chrome": "vite dev --config vite.chrome.config.ts",
    "watch:lib": "vite build --config vite.inject.config.ts --watch",
    "dist": "run-s build",
    "deploy": "run-s clean build",
    "release": "xs bump,test && run-s deploy && xs git-push",
    "prettier": "prettier . --write"
  },
  "pre-commit": [
    "lint",
    "types",
    "prettier"
  ],
  "eslintConfig": {
    "extends": "@pixi/eslint-config"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-collapsible": "^1.0.3",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toggle": "^1.0.3",
    "@radix-ui/react-toggle-group": "^1.0.4",
    "@radix-ui/react-tooltip": "^1.0.7",
    "react": "^18.2.0",
    "react-accessible-treeview": "^2.8.3",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "smoothie": "^1.36.1",
    "styled-components": "^6.1.8",
    "webextension-polyfill": "^0.10.0",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "^2.0.0-beta.23",
    "@pixi/eslint-config": "^4.1.0",
    "@pixi/extension-scripts": "^2.4.0",
    "@pixi/webdoc-template": "^1.5.5",
    "@types/chrome": "^0.0.253",
    "@types/node": "^18.17.1",
    "@types/react": "^18.2.39",
    "@types/react-dom": "^18.2.17",
    "@types/webextension-polyfill": "^0.10.0",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "@vitejs/plugin-react-swc": "^3.0.1",
    "@webdoc/cli": "^2.2.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "fs-extra": "^11.2.0",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "patch-package": "^8.0.0",
    "pixi.js": "^8.0.0-rc.8",
    "pre-commit": "^1.2.2",
    "prettier": "^3.2.5",
    "rimraf": "^4.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3",
    "vite": "^4.5.0"
  },
  "peerDependencies": {
    "pixi.js": ">=8.0.0-0"
  },
  "extensionConfig": {
    "clean": [
      "screenshots",
      "deploy"
    ],
    "namespace": "PIXI.filters",
    "docsName": "PixiJS Dev Tools",
    "docsCopyright": "PixiJS Copyright Â© 2013-2024 PixiJS.",
    "docsGoogleAnalytics": "UA-103772589-3",
    "docsTitle": "PixiJS Dev Tools API Documentation",
    "docsDescription": "Documentation for PixiJS Dev Tools library",
    "docsKeyword": "docs, documentation, pixi, pixijs, devtools, webgl, webgpu",
    "lint": [
      "src",
      "examples",
      "scripts"
    ],
    "deployFiles": "{dist,examples,docs}/**"
  }
}
